{
  "name": "@mcplug/client",
  "private": false,
  "version": "1.0.69",
  "description": "Server for mcplug",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "dist/bin.js",
  "license": "MIT",
  "author": "mcplug",
  "homepage": "https://github.com/mcplug/sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcplug/sdk.git"
  },
  "scripts": {
    "build": "tsup && chmod 755 dist/bin.js",
    "build-dev": "tsup --watch",
    "pub": "npm run build && npx -y verinc && npm publish",
    "test": "vitest --ui"
  },
  "dependencies": {
    "@mcplug/server": "latest",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@vitest/ui": "^3.0.9",
    "ai": "^4.2.6",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "esm-env": "^1.2.2",
    "jwt-decode": "^4.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.3",
    "vitest": "~3.0.9"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    "./ai": {
      "types": "./dist/ai/index.d.ts",
      "require": "./dist/ai/index.js",
      "import": "./dist/ai/index.js"
    }
  },
  "typeVersion": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ]
    },
    "publishConfig": {
      "access": "public"
    }
  }
}
